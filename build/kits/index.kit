		<section id="player">
		</section>
		<section id="intro">
			<div class="inner cover">
				<h1 class="cover-heading">#mcconnelling</h1>
				<p class="lead">Jon's on break, but the game is still on.</p>
				<p class="lead">
					<a href="#" class="btn btn-lg btn-default">Learn more</a>
					<a href="#" id="get-started" class="btn btn-lg btn-default start-editor">Get started</a>
				</p>
			</div>
		</section>
		<a href='/' id='solo-make' class='btn btn-lg btn-default start-editor'>Make Your Own</a>

		<section id="editor">
			<div class="timeline">
				<!-- <span class="instructions">Drag your #mcconnelling clips from below and remix!</span><br> -->
				<div class="demo">
				    <ul id="sortable">
				        <li class="timeline-clip 1" data-clip="1"><img src="_/img/1.jpg" alt=""></li>
				        <li class="timeline-clip 2" data-clip="2"><img src="_/img/2.jpg" alt=""></li>
				        <li class="timeline-clip 3" data-clip="3"><img src="_/img/3.jpg" alt=""></li>
				        <li class="timeline-clip 4" data-clip="4"><img src="_/img/4.jpg" alt=""></li>
				        <li class="timeline-clip 8" data-clip="8"><img src="_/img/8.jpg" alt=""></li>
				        <li class="timeline-clip 12" data-clip="12"><img src="_/img/12.jpg" alt=""></li>
				    </ul>
				</div>
			</div>
			<div class="inner">
				<div class="clips">
					<h2>Click on clips to add to timeline.</h2>
					<h2>Drag & drop to rerrange.</h2>
					<img class='gallery-clip' data-clip='1' class="1" src="_/img/1.jpg" alt="">
					<img class='gallery-clip' data-clip='2' class="2" src="_/img/2.jpg" alt="">
					<img class='gallery-clip' data-clip='3' class="3" src="_/img/3.jpg" alt="">
					<img class='gallery-clip' data-clip='4' class="4" src="_/img/4.jpg" alt="">
					<img class='gallery-clip' data-clip='8' class="8" src="_/img/8.jpg" alt="">
					<img class='gallery-clip' data-clip='12' class="12" src="_/img/12.jpg" alt="">
				</div>
				<div id='audio'>
					<input class="editor-title" id="title" value="" placeholder="Give your work of art a name">
					<br>
					<input class="editor-url audio" name="audio" id="audio-url" value="" placeholder="Paste audio URL here (YouTube only)">
					<input class="editor-time audio"  name="audio_start" id="audio-start" type="number" value="" placeholder="sec">
				</div>
				<button id='save'>Share it with the World</button>
			</div>
		</section>

